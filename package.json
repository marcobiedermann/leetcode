{
  "name": "leetcode",
  "version": "1.0.0",
  "description": "My attempt of solving Leetcode problems.",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint": "npm run lint:js",
    "test": "vitest",
    "test:ci": "npm run test -- run --coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/leetcode.git"
  },
  "keywords": [
    "algorithms",
    "leetcode",
    "problems",
    "problem-solving",
    "solutions"
  ],
  "author": "Marco Biedermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcobiedermann/leetcode/issues"
  },
  "homepage": "https://github.com/marcobiedermann/leetcode#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vitest": "^1.5.0"
  },
  "lint-staged": {
    "**/*": "npm run format"
  }
}
